{
  "name": "redux-devtools-extension",
  "version": "2.2.0",
  "description": "DevTools extension for Redux",
  "scripts": {
    "start": "gulp",
    "prebuild:extension": "npm run lint && npm run test",
    "prebuild:firefox": "npm run lint",
    "prebuild:examples": "npm run lint",
    "build:extension": "cross-env BABEL_ENV=production gulp build:extension",
    "build:firefox": "cross-env BABEL_ENV=production gulp build:firefox",
    "build:examples": "babel-node examples/buildAll.js",
    "compress:extension": "npm run build:extension && gulp compress:extension",
    "compress:firefox": "npm run build:firefox && gulp compress:firefox",
    "clean": "rm -rf build/ && rm -rf dev/",
    "lint": "eslint .",
    "test:app": "cross-env BABEL_ENV=test mocha --require test/app/setup.js --recursive test/app",
    "test:chrome": "cross-env BABEL_ENV=production gulp build:extension && gulp test:chrome",
    "test": "npm run test:app && npm run test:chrome"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalmoxisus/redux-devtools-extension"
  },
  "homepage": "https://github.com/zalmoxisus/redux-devtools-extension",
  "authors": [
    "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
    "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "chromedriver": "^2.19.0",
    "co-mocha": "^1.1.2",
    "cross-env": "^1.0.8",
    "enzyme": "^2.3.0",
    "eslint": "^1.7.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.2.3",
    "expect": "^1.20.1",
    "gulp": "^3.9.0",
    "gulp-jade": "1.1.0",
    "gulp-mocha": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.6",
    "gulp-zip": "^3.0.2",
    "jsdom": "^9.3.0",
    "mocha": "^2.3.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "selenium-webdriver": "^2.53.3",
    "sinon-chrome": "^1.1.2",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "jsan": "^3.1.1",
    "lodash": "^4.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-icons": "^2.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-instrument": "^1.0.1",
    "remotedev-app": "^0.5.0-alpha-2"
  }
}
